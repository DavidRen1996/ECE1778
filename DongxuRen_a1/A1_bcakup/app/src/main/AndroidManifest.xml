<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dyson.a1_backup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="A1"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.dyson.a1_backup.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dyson.a1_backup.SecondActivity"
            android:label="Register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.dyson.a1_backup.ThirdActivity"
            android:label="Content"
            android:exported="true">


        </activity>
    </application>

</manifest>